{
  "name": "create-interchain-app-project",
  "version": "0.0.1",
  "publishConfig": {
    "access": "restricted"
  },
  "private": true,
  "scripts": {
    "build": "lerna run prepare --parallel",
    "buidl": "lerna run buidl --parallel",
    "bootstrap": "pnpm -w install",
    "lint": "lerna run lint",
    "format": "lerna run format",
    "test": "lerna run test --stream",
    "update": "pnpm -w up -i -L",
    "clear:node_modules": "find . -name 'node_modules' -type d -prune -print | xargs rm -rf"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@pyramation/babel-preset-env": "0.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "eslint": "8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "lerna": "4.0.0",
    "prettier": "2.8.0",
    "copyfiles": "2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperweb-io/create-interchain-app"
  },
  "packageManager": "pnpm@9.7.0"
}